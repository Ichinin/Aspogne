<UserControl x:Class="Environment.Square"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Environment"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Border BorderBrush="Black" BorderThickness="1">
        <Grid x:Name="SquareGrid" Background="White">
            <Grid.RowDefinitions>
                <RowDefinition></RowDefinition>
                <RowDefinition></RowDefinition>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
            </Grid.ColumnDefinitions>
        
            <Image Grid.Row="0" Grid.Column="0" Source="Aspiro.png"></Image>
            <Image x:Name="Dust" Grid.Row="1" Grid.Column="0" Source="Dust.jpg"></Image>
            <Image Grid.Row="1" Grid.Column="1" Source="Jewel.png"></Image>
        </Grid>
    </Border>
</UserControl>
